{
  "env": {
    "GRIDSOME_UNSPLASH_ACCESS_KEY": "@unsplash-access-key",
    "GRIDSOME_GOOGLE_ANALYTICS_ID": "@og-image-builder-analytics"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}